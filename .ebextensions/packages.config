container_commands:
  01_update_bundler:
    command: "gem update bundler"
  02_bundle_install:
    command: "bundle install"
  03_precompile_assets:
    command: "bundle exec rails assets:precompile"
  04_precompile_webpacker:
    command: "bundle exec rails webpacker:compile"
  05_fake_to_check:
    command: "bundle exec fakecommand"
